#include <iostream>

using namespace std;
int main (){
    
int num1{},num2{};

cout<<"enter two integer seprated by space ";
cin>>num1>>num2;

if(num1 != num2){
    cout<<"largest"<<((num1>num2) ? num1 : num2)<<endl;
    cout<<"smallest " <<((num1 < num2) ? num1 : num2)<<endl;
}else{
    cout<<"the number are same ";
}
cout<<endl;
return 0;    
}
